<%= simple_form_for(@wtopic) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :wforum_id %>
    <%= f.input :user_id %>
    <%= f.input :name %>
    <%= f.input :last_poster_id %>
    <%= f.input :last_post_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
