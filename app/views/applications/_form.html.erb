<%= simple_form_for(@application) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :application_id %>
    <%= f.input :job_id %>
    <%= f.input :user_id %>
    <%= f.input :application_cover_note %>
    <%= f.input :cv %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
